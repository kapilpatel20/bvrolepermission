{
    "name" : "kapilpatel20/bvrolepermission",
    "description" : "A Role Base Permission with fos userbundle",
    "type" : "symfony-bundle",
    "homepage": "https://github.com/kapilpatel20/bvrolepermission",
    "authors" : [{
        "name" : "Kapil",
        "email" : "kapil.patel@brainvire.com"
    }],
    "keywords" : [
        "Role Base permission bundle"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
      "friendsofsymfony/user-bundle": "~2.0@dev"
    },
    "minimum-stability": "dev",
    "autoload" : {
        "psr-4": { "": "src/" }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.3.9"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}
